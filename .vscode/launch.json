    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "(gdb) fast_test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/gcn_sim",
            "args": [
                "-input=131072",
                "-output=4194304",
                "-edge=2097152",
                "-agg=16777216",
                "-hash-table-size=1048576",
                "-aggCores=512",
                "-systolic-rows=32",
                "-systolic-cols=128",
                "-graph-name=amazon_cs_graph_data",
                "-dram-name=HBM-config.cfg",
                "-model=gsc",
                "-ignore-neighbor=0",
                "-ignore-self=0",
                "-enable-feature-sparsity=0",
                "-mem-sim=ramulator",
                "-dram-freq=0.5",
                "-enable-dense-window",
                "-enable-fast-sched",
                "-short-large-divider=2",
                // "-enable-ideal-selection",
                "-enable-sequential-selection",
                // "-debug",
                // "-enable-outer-list",
                "-enable-ideal-hash1", 
                "-outer-name=lists/amazon_cs_graph_data.graph.bfs.30.seqresult",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/run_outer_queue",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/home/linuxbrew/.linuxbrew/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) sliding",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/gcn_sim",
            "args": [
                "-input=131072",
                "-output=4194304",
                "-edge=2097152",
                "-agg=16777216",
                "-hash-table-size=1048576",
                "-aggCores=512",
                "-systolic-rows=32",
                "-systolic-cols=128",
                "-graph-name=amazon_cs_graph_data",
                "-dram-name=HBM-config.cfg",
                "-model=gsc",
                "-ignore-neighbor=0",
                "-ignore-self=0",
                "-enable-feature-sparsity=0",
                "-mem-sim=ramulator",
                "-dram-freq=0.5",
                // "-enable-dense-window",
                // "-enable-fast-sched",
                "-short-large-divider=2",
                // "-enable-ideal-selection",
                // "-enable-sequential-selection",
                // "-debug",
                "-enable-outer-list",
                "-enable-ideal-hash", 
                "-outer-name=lists/amazon_cs_graph_data.graph.bfs.30.seqresult",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/run_outer_queue",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/home/linuxbrew/.linuxbrew/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) controller test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/gcn_sim_test",
            "args": [
                "-e",
                "-b",
                "controller_test"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) test dramsim3",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build-tst/gcn_sim",
            "args": [
                "-input=131072 ",
                "-output=4194304",
                "-edge=2097152",
                "-agg=4194304",
                "-aggCores=512",
                "-systolic-rows=32",
                "-systolic-cols=128",
                "-graph-name=acm",
                "-dram-name=HBM2_8Gb_x128.ini",
                "-model=gsc",
                "-ignore-neighbor=0",
                "-ignore-self=0",
                "-mem-sim=dramsim3",
                "-dram-freq=0.5",
                "-enable_valid_node_only"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/run",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}