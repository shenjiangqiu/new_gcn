{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) test dramsim3",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build-tst/gcn_sim",
            "args": ["-input=131072 ","-output=4194304",
                "-edge=2097152","-agg=4194304","-aggCores=512",
                "-systolic-rows=32","-systolic-cols=128",
                "-graph-name=acm",
                "-dram-name=HBM2_8Gb_x128.ini", "-model=gsc",
                "-ignore-neighbor=0","-ignore-self=0",
                 "-mem-sim=dramsim3","-dram-freq=0.5", "-enable_valid_node_only"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/run",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}